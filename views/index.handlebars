<main>
    {{> navbar }}
    {{> header }}


<div class="row">
    <div class="col s6 center-align">
        <form>
            <p id=email-login>Email:
                <input type="text" name="username" size="15" maxlength="30" />
            </p>
        </form>
    </div>
    <div class="col s6 center-align">
        <form>
            <p id=password-login>Password:
                <input type="password" name="password" size="15" maxlength="30" />
            </p>
        </form>
    </div>
</div>
</div>
<div class="row">
    <div class="col s12 m3 l5"></div>
    <div class="col s6 m3 l1">
        <a href="/signup" class="waves-effect waves-light btn"><i class="material-icons left">camera_front</i>SIGN
            UP</a>
    </div>
    <div class="col s6 m3 l2">
        <button class="waves-effect waves-light btn"><i class="material-icons right"
                id="enter">camera_roll</i>ENTER</button>
    </div>
    <div class="col s12 m3 l4"></div>
</div>


</main>